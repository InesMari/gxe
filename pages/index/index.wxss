page{
  /* padding-bottom: 160rpx;
  height: calc(100% + 160rpx); */
  height: 100%;
  box-sizing: border-box;
}
.header{
  height: 70rpx;
  padding: 10rpx 24rpx;
}
.header .img{
  width: 50rpx;
  margin-top: 10rpx;
}
.header .text{
  line-height: 70rpx;
  margin-left: 14rpx;
}
.header .text text{
  font-weight: bold;
  font-size: 26rpx;
  color: #1089db;
}
.sroll{
  height: 100%;
}
.banner{
  position: relative;
}
.dataList{
  background: #fff;
  border-radius: 10rpx;
  margin:30rpx 24rpx;
  border:1rpx solid #dee2e9;
  position: relative;
}
.dataList .xf{
  font-size: 26rpx;
  font-weight: bold;
  border-radius: 50%;
  width: 200rpx;
  height: 200rpx;
  line-height: 200rpx;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -100rpx 0 0 -100rpx;
  border:1rpx solid #dee2e9;
  background: #fff;
}
.dataList .item{
  float: left;
  box-sizing: border-box;
  width: 50%;
  padding: 30rpx 0;
  text-align: center;
  height: 240rpx;
  box-sizing: border-box;
  border-right: 1rpx dashed #dee2e9;
  border-bottom: 1rpx dashed #dee2e9;
  font-weight: bold;
}
.dataList .item:nth-child(2n+1){
  border-right: none;
}
.dataList .item .title{
  margin-top: 50rpx;
  font-size: 28rpx;
}
.dataList .item .num{
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #1089db;
}